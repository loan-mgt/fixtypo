{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fixtypo",
  "version": "0.1.0",
  "identifier": "com.loanmgt.fixtypo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fixtypo",
        "width": 600,
        "height": 600,
        "visible": false
      },
      {
        "label": "duck",
        "title": "",
        "url": "duck.html",
        "width": 100,
        "height": 100,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "visible": false,
        "resizable": false,
        "shadow": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}